  <customWorkspace>/opt/hudson/buildouts/eea-buildout-hudson/extras/Products.EEAContentTypes</customWorkspace>
  <builders>
    <hudson.tasks.Shell>
      <command>cd ${WORKSPACE}
../../bin/coverage run ../../bin/xmltestreport -v -vv --nowarning -s Products.EEAContentTypes
</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>cd ${WORKSPACE}
../../bin/report xml --include=*Products/EEAContentTypes*
</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>cd ${WORKSPACE}
mkdir -p xmltestreport
cp ../../parts/xmltestreport/testreports/*Products.EEAContentTypes*.xml xmltestreport/
cp ../../parts/xmltestreport/testreports/*Products-EEAContentTypes*.xml xmltestreport/</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>cd ${WORKSPACE}
../../bin/zptlint-test</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>cd ${WORKSPACE}
../../bin/pyflakes-test</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>cd ${WORKSPACE}
../../bin/pylint-test</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>cd ${WORKSPACE}
../../bin/jslint-test Products</command>
    </hudson.tasks.Shell>
  </builders>
