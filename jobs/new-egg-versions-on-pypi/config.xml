  <customWorkspace>/opt/hudson/buildouts/www.eea.europa.eu-hudson/</customWorkspace>
  <builders>
    <hudson.tasks.Shell>
      <command>cd ${WORKSPACE}
echo &quot;Now checking for new minor versions&quot;
bin/checkversions versions.cfg -l 2 -v | grep &quot;was:&quot;
echo &quot;Now checking for any version&quot;
bin/checkversions versions.cfg -v | grep &quot;was:&quot;</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
